## Screenshots ##
sudo pacman -Sy grim
sudo pacman -Sy swappy
sudo pacman -Sy slurp

## Creates Home directories
mkdir ~/Screenshots ~/Downloads ~/VS-Workspaces

## Yay Packages ##
## swww ##
yay -S swww
swww init

## nordvpn ##
yay -S nordvpn-bin
groupadd -r nordvpn
gpasswd -a <username> nordvpn
systemctl enable nordvpnd.service
systemctl start nordvpnd.service
